                                         <item row="4" column="1">
                                          <widget class="QPushButton" name="TEST__POST_PROCESSING__MEASURE_PROPERTIES__REMOVE_BY_PROPERTIES__ENABLE__INST_SEG__INFO">
                                           <property name="minimumSize">
                                            <size>
                                             <width>30</width>
