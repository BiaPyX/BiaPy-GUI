                                         </item>
                                         <item row="15" column="2">
                                          <widget class="QComboBox" name="TEST__POST_PROCESSING__MEASURE_PROPERTIES__REMOVE_BY_PROPERTIES__ENABLE__DET__INPUT">
                                           <property name="minimumSize">
                                            <size>
                                             <width>200</width>
                                             <height>30</height>
                                            </size>
